{{ header|raw }}
<div class="card" id="rating-page">
    <div class="header">
        <h4 class="title">{{ text('ratingTable') }}</h4>
    </div>
    <div class="content rating-content">
        {% if ratingTable %}
        <div class="table-responsive table-full-width">
            {{ ratingTable|raw }}
        </div>
        {% else %}
        <div class="alert alert-danger">{{ text('ratingClosed') }}</div>
        {% endif %}
    </div>
</div>
{{ footer|raw }}